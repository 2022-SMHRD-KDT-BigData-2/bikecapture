<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.bike.mapper.BikeMapper">
	<select id="join" resultType="kr.bike.entity.user">
	    select * from book
	    order by num
	</select>
	<select id="login" resultType="kr.bike.entity.user">
	    select * from book
	    order by num desc
	</select>
</mapper>